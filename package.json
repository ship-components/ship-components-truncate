{
  "name": "ship-components-truncate",
  "version": "0.1.3",
  "description": "Truncate comments and strings",
  "main": "src/Truncate.js",
  "scripts": {
    "start": "grunt server",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "test": "npm run lint && jest --coverage",
    "test:update": "npm run test -- -u",
    "test:watchAll": "npm run test -- --watchAll",
    "watch:test": "jest --coverage --watch",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v",
    "build": "npm test && grunt clean && grunt build",
    "travis": "npm install trevor && trevor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ship-components-truncate.git"
  },
  "engines": {
    "node": ">=4.0 <12.0"
  },
  "keywords": [
    "react",
    "es2015",
    "es2016",
    "es2017",
    "javascript",
    "javascript-library",
    "grunt",
    "webpack",
    "jest"
  ],
  "author": "David gong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/ship-components-truncate/issues"
  },
  "homepage": "git+https://github.com/ship-components/ship-components-truncate#readme",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "immutable": "^3.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "coveralls": "^3.0.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-ship-components": "^1.5.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-coveralls": "^2.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-webpack": "^3.1.3",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.1.0",
    "package-banner": "^0.1.2",
    "postcss-calc": "^7.0.1",
    "postcss-color-function": "^4.1.0",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "prop-types": "^15.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-dev-server": "^3.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!ship-components-*)"
    ]
  },
  "babelJest": {
    "include": [
      "src/**/*.@(js|jsx|es6)",
      "<rootDir>/node_modules"
    ]
  }
}
